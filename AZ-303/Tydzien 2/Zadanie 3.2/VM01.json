{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_Contoso_WWW_VM01_name": {
            "defaultValue": "Contoso-WWW-VM01",
            "type": "String"
        },
        "disks_Contoso_WWW_VM01_disk1_07aafc34c78d4cf28d18bae712cdb1d4_externalid": {
            "defaultValue": "/subscriptions/31db1cf0-ddaf-40ef-9599-0f9dd0abd54c/resourceGroups/Contoso-RG/providers/Microsoft.Compute/disks/Contoso-WWW-VM01_disk1_07aafc34c78d4cf28d18bae712cdb1d4",
            "type": "String"
        },
        "networkInterfaces_contoso_www_vm01720_externalid": {
            "defaultValue": "/subscriptions/31db1cf0-ddaf-40ef-9599-0f9dd0abd54c/resourceGroups/Contoso-RG/providers/Microsoft.Network/networkInterfaces/contoso-www-vm01720",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2019-07-01",
            "name": "[parameters('virtualMachines_Contoso_WWW_VM01_name')]",
            "location": "westeurope",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_D2s_v3"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "Canonical",
                        "offer": "UbuntuServer",
                        "sku": "18.04-LTS",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Linux",
                        "name": "[concat(parameters('virtualMachines_Contoso_WWW_VM01_name'), '_disk1_07aafc34c78d4cf28d18bae712cdb1d4')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "storageAccountType": "Premium_LRS",
                            "id": "[parameters('disks_Contoso_WWW_VM01_disk1_07aafc34c78d4cf28d18bae712cdb1d4_externalid')]"
                        },
                        "diskSizeGB": 30
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_Contoso_WWW_VM01_name')]",
                    "adminUsername": "adminwww",
                    "linuxConfiguration": {
                        "disablePasswordAuthentication": false,
                        "provisionVMAgent": true
                    },
                    "secrets": [],
                    "allowExtensionOperations": true,
                    "requireGuestProvisionSignal": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_contoso_www_vm01720_externalid')]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true
                    }
                }
            }
        }
    ]
}
